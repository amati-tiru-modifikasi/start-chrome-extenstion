{
    "name": "Bulk Image Downloader",
    "description": "Download Every image in Browser Chrome",
    "version": "0.0.1",
    "permissions": ["contextMenus","activeTab"],
    "icons": {
        "16":"16.png",
        "48":"48.png",
        "64":"64.png",
        "128":"128.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://www.google.com/*"],
            "js": ["content.js"]
        }
    ],
    "manifest_version": 2
}