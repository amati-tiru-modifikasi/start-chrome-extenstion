{
    "name": "Bulk Image Downloader",
    "description": "Download Every image in Browser Chrome",
    "version": "0.0.1",
    "permissions": ["<all_urls>","tabs","activeTab"],
    "icons": {
        "16":"16.png",
        "48":"48.png",
        "64":"64.png",
        "128":"128.png"
    },
    "browser_action": {
        "default_popup": "popup/index.html"
    },
    "background": {
        "scripts": ["jquery.min.js","background.js"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery.min.js","content.js"]
        }
    ],
    "manifest_version": 2
}