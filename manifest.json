{
    "name": "My Second Extenstion Tutorial Followed",
    "description": "Another tutorial about create chrome extenstion",
    "version": "0.0.1",
    "permissions": ["contextMenus","activeTab"],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://www.google.com/*","*://www.youtube.com/*"],
            "js": ["index.js"]
        }
    ],
    "manifest_version": 2
}